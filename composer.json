{
    "name": "vajexal/js-object-parser",
    "description": "JS object parser",
    "license": "Unlicense",
    "require": {
        "php": "^8.0",
        "symfony/polyfill-mbstring": "^1.22"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Vajexal\\JsObjectParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vajexal\\JsObjectParser\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
